{
  "name": "@union/ts-components",
  "version": "0.0.40",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "yarn build && yarn storybook",
    "build": "yarn tsc && yarn dist",
    "build:watch": "concurrently \"yarn tsc -w\" \"yarn dist -w\"",
    "dist": "NODE_ENV=production rollup -c",
    "compile": "tsc -p .",
    "declarations": "tsc --emitDeclarationOnly",
    "prebuild": "rm -rf dist dist-transpiled",
    "prewebpack": "rm -rf dist dist-transpiled",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "storybook": "start-storybook --port 9001",
    "build:storybook": "build-storybook && yarn deploy",
    "deploy": "surge --domain playcore-storybook.surge.sh --project ./storybook-static",
    "generate": "schematics ./node_modules/@union/react-schematics/src/collection.json:react --debug=false",
    "release.prepare": "node scripts/prepare.js"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@styled-system/theme-get": "^5.1.2",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.13",
    "@types/react-modal": "^3.10.1",
    "@types/styled-components": "4.0.2",
    "@types/styled-system": "^5.1.4",
    "@types/styled-system__theme-get": "^5.0.0",
    "@types/swiper": "^5.2.0",
    "@union/react-schematics": "^0.0.0",
    "core-js": "2",
    "lodash": "^4.17.15",
    "polished": "^3.4.2",
    "react-modal": "^3.11.1",
    "styled-components": "^4.4.1",
    "styled-flex-component": "^3.0.0",
    "styled-system": "^5.1.4",
    "swiper": "^5.2.1",
    "tslib": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@rollup/plugin-alias": "^3.0.0",
    "@storybook/addon-a11y": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-viewport": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@types/node": "^12.12.14",
    "@types/react-dom": "^16.9.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.0",
    "execa": "^4.0.0",
    "fs-extra": "^8.1.0",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "react": "^16.10.2",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.10.2",
    "react-testing-library": "^8.0.1",
    "rollup": "^1.27.5",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-svgo": "^1.1.0",
    "rollup-plugin-virtual": "^1.0.1",
    "semver": "^7.1.1",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.2",
    "typescript-styled-plugin": "^0.15.0"
  },
  "peerDependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  }
}
